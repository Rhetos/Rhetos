<Project>
  <PropertyGroup>
    <LangVersion>12.0</LangVersion>
    <Company>Omega software</Company>
    <Copyright>Copyright (C) Omega software 2017</Copyright>
    <AssemblyVersion>6.1.0</AssemblyVersion>
    <FileVersion>6.1.0</FileVersion>
    <InformationalVersion>6.1.0-dev</InformationalVersion>
    <!-- The warning CA1014 is a project level warning that is not tied to a specific file so it can not be suppressed in .editorconfig -->
    <NoWarn>CA1014</NoWarn>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AnalysisMode>AllEnabledByDefault</AnalysisMode>
  </PropertyGroup>
</Project>