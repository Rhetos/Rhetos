<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Rhetos.MSBuild</id>
    <version>5.0.0-dev</version>
    <authors>Omega software</authors>
    <owners>Omega software</owners>
    <licenseUrl>http://opensource.org/licenses/AGPL-3.0</licenseUrl>
    <projectUrl>https://github.com/rhetos/rhetos</projectUrl>
    <description>MSBuild integration with Rhetos CLI, for Rhetos DSL framework.</description>
    <copyright>Copyright Omega software 2013</copyright>
    <tags>Rhetos</tags>
    <dependencies>
      <dependency id="Rhetos" version="[5.0.0-dev,5.1.0)" />
    </dependencies>
    <contentFiles>
      <files include="any/any/rhetos.exe" buildAction="None" copyToOutput="true" />
      <files include="any/any/rhetos.dll" buildAction="None" copyToOutput="true" />
      <files include="any/any/rhetos.pdb" buildAction="None" copyToOutput="true" />
      <files include="any/any/rhetos.deps.json" buildAction="None" copyToOutput="true" />
      <files include="any/any/rhetos.runtimeconfig.json" buildAction="None" copyToOutput="true" />
      <files include="any/any/nlog.config" buildAction="None" copyToOutput="true" />
    </contentFiles>
  </metadata>
  <files>
    <file src="RhetosCli\bin\Debug\net5.0\*.dll" target="tools" />
    <file src="RhetosCli\bin\Debug\net5.0\*.pdb" target="tools" />
    <file src="RhetosCli\bin\Debug\net5.0\rhetos.exe" target="tools" />
    <file src="RhetosCli\bin\Debug\net5.0\nlog.config" target="tools" />
    <file src="RhetosCli\bin\Debug\net5.0\rhetos.deps.json" target="tools" />
    <file src="RhetosCli\bin\Debug\net5.0\rhetos.runtimeconfig.json" target="tools" />
    <file src="RhetosCli\bin\Debug\net5.0\runtimes\**\*" target="tools/runtimes" />

    <file src="MsBuildIntegration\Rhetos.MSBuild.targets" target="build" />
    <file src="MsBuildIntegration\Rhetos.MSBuild.props" target="build" />

    <file src="RhetosVSIntegration\bin\Debug\netstandard2.0\*.dll" target="build" />
    <file src="RhetosVSIntegration\bin\Debug\netstandard2.0\*.pdb" target="build" />
    <file src="RhetosVSIntegration\bin\Debug\netstandard2.0\*.json" target="build" />

    <file src="RhetosCli\bin\Debug\net5.0\rhetos.exe" target="contentFiles/any/any" />
    <file src="RhetosCli\bin\Debug\net5.0\rhetos.dll" target="contentFiles/any/any" />
    <file src="RhetosCli\bin\Debug\net5.0\rhetos.pdb" target="contentFiles/any/any" />
    <file src="RhetosCli\bin\Debug\net5.0\rhetos.deps.json" target="contentFiles/any/any" />
    <file src="RhetosCli\bin\Debug\net5.0\rhetos.runtimeconfig.json" target="contentFiles/any/any" />
    <file src="RhetosCli\bin\Debug\net5.0\nlog.config" target="contentFiles/any/any" />
  </files>
</package>
